{
  "expo": {
    "name": "Pride One",
    "slug": "prideone",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "pridenotificationapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification.png",
          "sounds": [
            "./assets/sounds/ship_bell.wav"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.pridetradingconsultancy.prideone",
      "versionCode": 1,
      "notification": {
        "icon": "./assets/images/notification.png"
      }
    },
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.pridetradingconsultancy.prideone",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "pridenotificationapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "24f7e739-2309-4aa5-b6bb-d2c808784e55"
      }
    }
  }
}
